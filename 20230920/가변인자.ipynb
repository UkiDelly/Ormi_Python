   ],
   "source": [
    "def print_all(**kwargs):\n",
    "    for i in kwargs:\n",
    "        print(kwargs[i])\n",
    "\n",
    "\n",
    "d = {\"one\": 1, \"two\": 2, \"three\": 3}\n",
    "print_all(**d)"
   ]
  }
